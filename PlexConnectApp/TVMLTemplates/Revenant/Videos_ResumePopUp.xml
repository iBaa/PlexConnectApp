<?xml version="1.0" encoding="UTF-8" ?>
{{XML()}}{{
<document>
    <alertTemplate theme="light">
			<button onSelect="Presenter.closeContext(); videoPlayer.play('{{PMSID()}}', '{{PATH()}}', 'resume')">
       	<text>{{TEXT(Continue watching from: )}}{{DURATION_HMS({{VAL(MediaContainer/Video/viewOffset:)}})}}</text>
      </button>
      <button onSelect="Presenter.closeContext(); videoPlayer.play('{{PMSID()}}', '{{PATH()}}')">
       	<text>{{TEXT(Play from beginning)}}</text>
      </button>
    </alertTemplate>
</document>
}}