<?xml version="1.0" encoding="UTF-8" ?>
{{XML()}}{{
<document>
  <head>
      <style>
      </style>
  </head>
    <alertTemplate>
      <title><![CDATA[{{VAL(MediaContainer/Video/title)}}]]></title>

			<button onSelect="swiftInterface.sendPmsPath('{{PMSID()}}', '/:/scrobble?key={{PATH()}}&amp;identifier=com.plexapp.plugins.library'); Presenter.closeContext();">
       	<text>{{TEXT(Mark as Watched)}}</text>
      </button>
      <button onSelect="swiftInterface.sendPmsPath('{{PMSID()}}', '/:/unscrobble?key={{PATH()}}&amp;identifier=com.plexapp.plugins.library'); Presenter.closeContext();">
       	<text>{{TEXT(Mark as Unwatched)}}</text>
      </button>
    </alertTemplate>
</document>
}}
